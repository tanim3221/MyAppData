[GGSEMC_Install.NT]
CopyFiles=GGSEMC_CopyFiles

[GGSEMC_Install.NT.HW]
AddReg=GGSEMC_Install.NT.HW.AddReg

[GGSEMC_Install.NT.Services]
Addservice=ggsemc, 0x00000002, GGSEMC_Install.Service
Addservice=ggflt,, GGSEMC_Install.Filter_Service

[GGSEMC_Install.Service]
DisplayName=%SvcDesc%
ServiceType=1
StartType=3
ErrorControl=1
ServiceBinary=%12%\ggsemc.sys
AddReg=GGSEMC_Install.Service_AddReg

[GGSEMC_Install.Service_AddReg]
HKR, "Parameters", "NextUSBPortId", 0x00010003, 1
HKR, "Parameters\SLNames", "VID_0FCE&PID_ADDE", 0x00000000, "EPIUSB"
HKR, "Parameters\SLNames", "VID_0BDB&PID_100E", 0x00000000, "EPIUSB"
HKR, "Parameters\SLNames", "VID_0FCE&PID_D064", 0x00000000, "HCB100"
HKR, "Parameters\SLNames", "VID_0FCE&PID_D083", 0x00000000, "HCB120"
HKR, "Parameters\SLNames", "VID_0FCE&PID_D0A8", 0x00000000, "HCB100E"
HKR, "Parameters\SLNames", "VID_0FCE&PID_D0AA", 0x00000000, "HCB105"
HKR, "Parameters\SLNames", "VID_0FCE&PID_D0F9", 0x00000000, "HCB108"
HKR, "Parameters\SLNames", "VID_0FCE&PID_D104", 0x00000000, "MBS200"
HKR, "Parameters\SLNames", "VID_0FCE&PID_D101", 0x00000000, "AB900"
HKR, "Parameters\SLNames", "VID_0FCE&PID_F0AA", 0x00000000, "SEMCDFU"
HKR, "Parameters\SLNames", "VID_0FCE&PID_FFFF", 0x00000000, "SEMCDFU"
HKR, "Parameters\SLNames", "VID_0FCE&PID_F0F9", 0x00000000, "SEMCDFU"
HKR, "Parameters\SLNames", "VID_0FCE&PID_F104", 0x00000000, "SEMCDFU"
HKR, "Parameters\SLNames", "VID_0FCE&PID_F101", 0x00000000, "SEMCDFU"
HKR, "Parameters\SLNames", "VID_0FCE&PID_F0FA", 0x00000000, "SEMCDFU"
HKR, "Parameters\PNCheck", "VID_0FCE&PID_ADDE", 0x00010001, 0x00000001
HKR, "Parameters\PNCheck", "VID_0BDB&PID_100E", 0x00010001, 0x00000001
HKR, "Parameters\SLNames", "VID_0FCE&PID_F106", 0x00000000, "X1Boot"
HKR, "Parameters\SLNames", "VID_0FCE&PID_D14C", 0x00000000, "ETSUSB"

[GGSEMC_Install.Filter_Service]
DisplayName=%SvcFltDesc%
ServiceType=1
StartType=3
ErrorControl=0
ServiceBinary=%12%\ggflt.sys

[GGSEMC_Install.NT.HW.AddReg]
HKR,, "LowerFilters", 0x00010000, "ggflt"

[GGSEMC_CopyFiles]
ggsemc.sys
ggflt.sys

[GGSEMC_Install.NT.CoInstallers]
AddReg=WDFCoInstaller_AddReg
CopyFiles=WDFCoInstaller_CopyFiles

[WDFCoInstaller_CopyFiles]
WdfCoInstaller01007.dll

[WDFCoInstaller_AddReg]
HKR,,CoInstallers32,0x00010000,"WdfCoInstaller01007.dll,WdfCoInstaller"

[GGSEMC_Install.NT.Wdf]
KmdfService=ggsemc,GGSEMC_WDFSection
KmdfService=ggflt,GGSEMC_WDFSection

[GGSEMC_WDFSection]
KmdfLibraryVersion=1.7

